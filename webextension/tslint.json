{
	"extends": [
		"tslint-react",
		"tslint-config-airbnb"
	],
	"rules": {
		"align": false,
		"import-name": false,
		"jsx-boolean-value": [
			true,
			"never"
		],
		"jsx-no-multiline-js": false,
		"jsx-no-lambda": false,
		"max-line-length": false,
		"no-increment-decrement": false,
		"ter-indent": [
			true,
			"tab",
			{
				"SwitchCase": 1
			}
		],
		"variable-name": [
			true,
			"allow-leading-underscores",
			"allow-pascal-case"
		]
	}
}

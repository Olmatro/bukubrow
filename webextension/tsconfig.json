{
	"compilerOptions": {
		"module": "es6",
		"allowSyntheticDefaultImports": true,
		"moduleResolution": "node",
		"target": "es2015",
		"strict": true,
		"sourceMap": true,
		"lib": ["esnext", "dom"],
		"jsx": "react",
		"baseUrl": "./src/",
		"paths": {
			"Assets/*": [
				"./assetsBundledOnly/*"
			],
			"Components/*": [
				"./components/*"
			],
			"Modules/*": [
				"./modules/*"
			]
		}
	}
}

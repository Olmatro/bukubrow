{
	"compilerOptions": {
		"module": "es6",
		"allowSyntheticDefaultImports": true,
		"moduleResolution": "node",
		"target": "es2015",
		"strict": true,
		"sourceMap": true,
		"lib": ["esnext", "dom"],
		"jsx": "react",
		"baseUrl": "./src/",
		"paths": {
			"Assets/*": [
				"./assetsBundledOnly/*"
			],
			"Comms/*": [
				"./comms/*"
			],
			"Components/*": [
				"./components/*"
			],
			"Containers/*": [
				"./containers/*"
			],
			"Hooks/*": [
				"./hooks/*"
			],
			"Modules/*": [
				"./modules/*"
			],
			"Pages/*": [
				"./pages/*"
			],
			"Store": [
				"./store/index"
			],
			"Store/*": [
				"./store/*"
			]
		}
	}
}
